{
  "name": "nextcloud/openapi-extractor",
  "require": {
    "php": "^8.1",
    "ext-simplexml": "*",
    "nikic/php-parser": "^4.16",
    "adhocore/cli": "^v1.6",
    "phpstan/phpdoc-parser": "^1.23"
  },
  "require-dev": {
    "nextcloud/coding-standard": "^1.1"
  },
  "bin": [
    "generate-spec",
    "merge-specs"
  ],
  "autoload": {
    "psr-4": {
      "OpenAPIExtractor\\": "src"
    }
  },
  "scripts": {
    "cs:check": "php-cs-fixer fix --dry-run --diff",
    "cs:fix": "php-cs-fixer fix"
  }
}
