{
  "name": "nextcloud/openapi-extractor",
  "require": {
    "nikic/php-parser": "4.15.3",
    "adhocore/cli": "v1.2.0",
    "ext-simplexml": "*",
    "phpstan/phpdoc-parser": "1.16.0"
  },
  "bin": [
    "generate-spec",
    "merge-specs"
  ],
  "autoload": {
    "files": [
      "src/controller.php",
      "src/helpers.php",
      "src/logger.php",
      "src/openapi_type.php",
      "src/return_type.php",
      "src/route.php",
      "src/status_code.php"
    ]
  }
}
